package com.zhadui.common.entity;

public class UserExperience {

	private int userExperienceId;
	private String userExperienceCause;
	private int userExperiencePoints;
	
	public UserExperience(int userExperienceId, String userExperienceCause,
			int userExperiencePoints) {
		super();
		this.userExperienceId = userExperienceId;
		this.userExperienceCause = userExperienceCause;
		this.userExperiencePoints = userExperiencePoints;
	}
	
	public UserExperience() {
		super();
	}
	
	public int getUserExperienceId() {
		return userExperienceId;
	}
	public void setUserExperienceId(int userExperienceId) {
		this.userExperienceId = userExperienceId;
	}
	public String getUserExperienceCause() {
		return userExperienceCause;
	}
	public void setUserExperienceCause(String userExperienceCause) {
		this.userExperienceCause = userExperienceCause;
	}
	public int getUserExperiencePoints() {
		return userExperiencePoints;
	}
	public void setUserExperiencePoints(int userExperiencePoints) {
		this.userExperiencePoints = userExperiencePoints;
	}
	
	@Override
	public String toString() {
		return "{userExperienceId:" + userExperienceId
				+ ", userExperienceCause:" + userExperienceCause
				+ ", userExperiencePoints:" + userExperiencePoints + "}";
	}
	
}
