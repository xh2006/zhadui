package com.zhadui.common.entity;

public class DuierClass {

	private int duierClassId;
	private String duierClassName;
	private int duierClassFatherId;
	private int duierClassLayer;
	private int duierClassHotLevel;
	private String duierClassNote;
	private int duierClassExt1;
	private String duierClassExt2;
	
	public DuierClass() {
		super();
	}

	public DuierClass(int duierClassId, String duierClassName,
			int duierClassFatherId, int duierClassLayer,
			int duierClassHotLevel, String duierClassNote, int duierClassExt1,
			String duierClassExt2) {
		super();
		this.duierClassId = duierClassId;
		this.duierClassName = duierClassName;
		this.duierClassFatherId = duierClassFatherId;
		this.duierClassLayer = duierClassLayer;
		this.duierClassHotLevel = duierClassHotLevel;
		this.duierClassNote = duierClassNote;
		this.duierClassExt1 = duierClassExt1;
		this.duierClassExt2 = duierClassExt2;
	}

	public int getDuierClassId() {
		return duierClassId;
	}

	public void setDuierClassId(int duierClassId) {
		this.duierClassId = duierClassId;
	}

	public String getDuierClassName() {
		return duierClassName;
	}

	public void setDuierClassName(String duierClassName) {
		this.duierClassName = duierClassName;
	}

	public int getDuierClassFatherId() {
		return duierClassFatherId;
	}

	public void setDuierClassFatherId(int duierClassFatherId) {
		this.duierClassFatherId = duierClassFatherId;
	}

	public int getDuierClassLayer() {
		return duierClassLayer;
	}

	public void setDuierClassLayer(int duierClassLayer) {
		this.duierClassLayer = duierClassLayer;
	}

	public int getDuierClassHotLevel() {
		return duierClassHotLevel;
	}

	public void setDuierClassHotLevel(int duierClassHotLevel) {
		this.duierClassHotLevel = duierClassHotLevel;
	}

	public String getDuierClassNote() {
		return duierClassNote;
	}

	public void setDuierClassNote(String duierClassNote) {
		this.duierClassNote = duierClassNote;
	}

	public int getDuierClassExt1() {
		return duierClassExt1;
	}

	public void setDuierClassExt1(int duierClassExt1) {
		this.duierClassExt1 = duierClassExt1;
	}

	public String getDuierClassExt2() {
		return duierClassExt2;
	}

	public void setDuierClassExt2(String duierClassExt2) {
		this.duierClassExt2 = duierClassExt2;
	}

	@Override
	public String toString() {
		return "DuierClass [duierClassId=" + duierClassId + ", duierClassName="
				+ duierClassName + ", duierClassFatherId=" + duierClassFatherId
				+ ", duierClassLayer=" + duierClassLayer
				+ ", duierClassHotLevel=" + duierClassHotLevel
				+ ", duierClassNote=" + duierClassNote + ", duierClassExt1="
				+ duierClassExt1 + ", duierClassExt2=" + duierClassExt2 + "]";
	}
	
	
}
