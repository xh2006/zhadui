	<select id="getDuierInfoByDuierId" parameterType="int" resultMap="duierInfoResult">
		select * from duierInfo d,userInfo u,duierClass c where d.duierInfo_creator_id=u.userInfo_id and d.duierInfo_class_id=c.duierClass_id and duierInfo_id = #{id}
	</select>
	<select id="getDuierChatInfoByDuierId" parameterType="int" resultMap="duierChatInfoResult">
		select * from duierChatInfo,userInfo where duierChatInfo_duierId = #{id} and userInfo_id = duierChatInfo_talkerId
	</select>

