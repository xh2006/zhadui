	<select id="getDuierUndertakerByDuierId" parameterType="BigInteger"
		resultMap="DuierUndertakeInfoResult">
		select * from duierUndertakeInfo t,DuierInfo d,userInfo u
		where t.duierUndertakeInfo_duierId=d.duierInfo_id and
		t.duierUndertakeInfo_userId=u.userInfo_id and
		t.duierUndertakeInfo_beSelected=1 and
		t.duierUndertakeInfo_duierId=#{duierId}
	</select>

	<select id="getDuierUndertakeListByDuierId" parameterType="BigInteger"
		resultMap="DuierUndertakeInfoResult">
		select * from duierUndertakeInfo t,userInfo u where
		t.duierUndertakeInfo_userId=u.userInfo_id and
		t.duierUndertakeInfo_duierId=#{duierId}
	</select>

	<update id="updateDuierUndertakeForAppoint" parameterType="map">
		update duierUndertakeInfo set
		duierUndertakeInfo.duierUndertakeInfo_beSelected=1 where
		duierUndertakeInfo.duierUndertakeInfo_duierId=#{duierId} and
		duierUndertakeInfo.duierUndertakeInfo_userId=#{userId}
	</update>

	<insert id="addDuierUndertakeInfo" parameterType="DuierUndertakeInfo">
		insert into
		duierUndertakeInfo(duierUndertakeInfo_duierId,duierUndertakeInfo_userId,duierUndertakeInfo_affair,duierUndertakeInfo_fee,duierUndertakeInfo_time,duierUndertakeInfo_beSelected,duierUndertakeInfo_ext1,duierUndertakeInfo_ext2)
		values(#{duierUndertakeInfoDuier.duierInfoId},#{duierUndertakeInfoUser.userInfoId},#{duierUndertakeInfoAffair},#{duierUndertakeInfoFee},#{duierUndertakeInfoTime},#{duierUndertakeInfoBeSelected},#{duierUndertakeInfoExt1},#{duierUndertakeInfoExt2})
	</insert>
